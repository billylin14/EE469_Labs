module forwardingUnit ( input logic [4:0] AaRF, AbRF, AwEX, AwMEM,
								output logic [1:0] DaSEL, DbSEL );
	
	if(AaRF == AwEX) 				DaSEL = 2'b0;
	else if (AaRF == AwMEM) 	DaSEL = 2'b01;
	else 								DaSEL = 2'b10;
	
	if(AbRF == AwEX) 				DbSEL = 2'b0;
	else if (AbRF == AwMEM) 	DbSEL = 2'b01;
	else 								DbSEL = 2'b10;
	
endmodule
